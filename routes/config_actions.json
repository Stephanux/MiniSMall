        "title": "Modification formateur",
        "libelle": "Modification",
        "del_label": "ce Formateur"
    },
    "POST/modifyFormateurSQL": {
        "controler": "sql/updateSQL",
        "sql_query": "UPDATE formateurs SET nom=?, prenom=?, specialite=?, prix_jour=? WHERE id_formateur=?",
        "params_query": [
            "nom",
            "prenom",
            "specialite",
            "prix_jour"
        ],
        "redirect": "/selectFormateursSQL"
    },
    "GET/deleteFormateurSQL": {
        "controler": "sql/deleteSQLById",
        "sql_query": "DELETE FROM formateurs WHERE id_formateur=?",
